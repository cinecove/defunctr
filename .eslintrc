{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "mocha": true,
    "browser": true
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": [
        "error", {
            "ignoreRestSiblings": true,
            "caughtErrors": "none"
        }
    ]
  }
}
